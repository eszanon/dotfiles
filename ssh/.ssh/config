Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/ssh-eb-bitbucket
    IdentitiesOnly yes
    AddKeysToAgent yes
    # Suppress post-quantum warning (server-side limitation)
    LogLevel ERROR

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes
    AddKeysToAgent yes
