# See https://git-scm.com/docs/git-config

[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
[init]
	defaultBranch = master
[pull]
  rebase = true

[submodule]
  fetchJobs = 16

[color "branch"]
  current  = magenta
  remote   = yellow
  upstream = green
  plain    = blue

[editor]
  command = code --wait
